extends layouts/main
include mixins/videoBlock

block content
  .header__search
    h3 Searching By #{searchingBy}
  .search__videos
    if videos.length === 0 
      h5 No Videos Found
    each item in videos
      +videoBlock({
        videoFile: item.videoFile,
        title: item.title,
        views: item.views,
        description: item.description,
        id: item.id
      })